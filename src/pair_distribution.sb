#!/bin/bash

#SBATCH --time=04:00:00
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=1
#SBATCH --mem=5gb
#SBATCH --output=pair_distribution.o%j
#SBATCH --error=pair_distribution.e%j

# change to the current working directory
cd ${SLURM_SUBMIT_DIR}
# setup programs
module purge
module load Conda/3

# comment -- run my python script sample.py
python pair_distribution.py

scontrol show job $SLURM_JOB_ID
